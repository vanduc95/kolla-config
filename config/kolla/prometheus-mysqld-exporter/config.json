{
    "command": "/opt/mysqld_exporter/mysqld_exporter --config.my-cnf /etc/prometheus/my.cnf --web.listen-address 10.53.101.10:9104",
    "config_files": [
        {
            "source": "/var/lib/kolla/config_files/my.cnf",
            "dest": "/etc/prometheus/my.cnf",
            "owner": "prometheus",
            "perm": "0600"
        }
    ],
    "permissions": [
        {
            "path": "/var/log/kolla/prometheus",
            "owner": "prometheus:kolla",
            "recurse": true
        }
    ]
}
